{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Debug Server",
			"command": "C:\\msys64\\ucrt64\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${workspaceFolder}\\src\\server\\*.cpp",
				"-o",
				"${workspaceFolder}\\build\\debug\\server.exe",
				"-I",
				"${workspaceFolder}\\include\\server",
				"-I",
				"C:\\Program Files (x86)\\wxWidgets\\include",
				"-I",
				"C:\\Program Files (x86)\\wxWidgets\\lib\\gcc_x64_dll\\mswud",
				"-L",
				"C:\\Program Files (x86)\\wxWidgets\\lib\\gcc_x64_dll",
				"-l",
				"wxmsw32ud_core",
				"-l",
				"wxbase32ud",
				"-l",
				"wxbase32ud_net",
				"-lws2_32"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: C:\\msys64\\ucrt64\\bin\\g++.exe"
